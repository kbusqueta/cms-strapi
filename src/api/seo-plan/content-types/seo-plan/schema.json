{
  "kind": "collectionType",
  "collectionName": "seo_plans",
  "info": {
    "singularName": "seo-plan",
    "pluralName": "seo-plans",
    "displayName": "SEO Plan",
    "description": "Plans de rédaction SEO avec cocons sémantiques"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "keyword": {
      "type": "string"
    },
    "planData": {
      "type": "json",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "active",
        "completed"
      ],
      "default": "draft"
    },
    "lastModified": {
      "type": "datetime"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "seoPlans"
    }
  }
}